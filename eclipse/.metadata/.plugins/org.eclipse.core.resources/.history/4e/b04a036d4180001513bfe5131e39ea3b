public class CreativeTabDigimonTools extends CreativeTabs {

	public CreativeTabDigimonTools(int p_i1853_1_, String p_i1853_2_) {
		super(p_i1853_1_, p_i1853_2_);
	}

	@Override
	public Item getTabIconItem() {
		return DCItem.chainsaw;
	}

}
